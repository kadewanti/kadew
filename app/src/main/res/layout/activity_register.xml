        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="151dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="98dp"
        android:layout_height="37dp"
        android:text="EMAIL"
